{
    "extends": "@apify/tsconfig",
    "compilerOptions": {
      "module": "ES2022",
      "target": "ES2022",
      "outDir": "dist",
      "noUnusedLocals": false,
      "lib": ["DOM"],
      "strictNullChecks": false,
      "noImplicitAny": false,
      "noUnusedParameters": false
    },
    "include": ["./src/**/*"]
